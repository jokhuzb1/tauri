{
  "$schema": "https://schema.tauri.app/config/2.0.0.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "identifier": "com.yourcompany.mytauriapp",
  "productName": "MyTauriApp",
  "version": "0.0.0",
  "bundle": {
    "active": true,
    "targets": ["msi", "deb", "app", "dmg"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/icon.ico",
      "icons/icon.icns"
    ],
    "externalBin": [],
    "resources": []
  },
  "app": {
    "windows": [
      {
        "title": "My Tauri App",
        "width": 1200,
        "height": 800,
        "resizable": true
      }
    ]
  }
}
